<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultadoSalario">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/idResultado" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="@+id/idVoltar" android:layout_marginTop="47dp"
            android:layout_marginLeft="15dp" android:layout_marginStart="15dp"/>
    <Button
            android:text="Voltar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/idVoltar" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="49dp" app:layout_constraintTop_toBottomOf="@+id/idResultado"
            app:layout_constraintEnd_toEndOf="parent"/>
</android.support.constraint.ConstraintLayout>